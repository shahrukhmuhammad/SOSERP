<script type="text/javascript">
    jQuery(document).ready(function ($) {
        if (document.documentElement.clientWidth > 979) {
            $('#overlay').hide();
            var timeout;
            $('#nav').hover(
                function () {
                    timeout = setTimeout(function () {
                        timeout = null;
                        $('#overlay').fadeIn();
                        $('#nav ul.nav-primary li ul').show();
                    }, 600);
                },
            function () {
                if (timeout) {
                    window.clearTimeout(timeout);
                    timeoutId = null;
                }
                else {
                    $('#overlay').fadeOut();
                    $('#nav ul.nav-primary li ul').hide();
                }
            });
        };
    });



