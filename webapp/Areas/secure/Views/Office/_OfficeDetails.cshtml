<script>$.each($q, function (i, f) { $(f) }); $q = null;</script>
<script>
    $(function () {
        function SuggestedBox() {
            $('#suggested-box').toggle(300, function () {
                $.get("/Secure/TaskManagement/_SuggestedList", function (d) {
                    $('#browse-box').hide();
                    $('#suggested-box').html(d);
                    filterDataTable("#tasks-dataTable", [0, 1, 2, 3, 4]);
                });
            });
        }
        $('.crmsearch').on('click', function () {
            $('#crm-box-panel').toggle(300, function () {
                $('#loading-mask-inner').show();
                $.get("/Secure/TaskManagement/_EmployeesList", function (d) {
                    $('#loading-mask-inner').hide();
                    $('#crm-box-panel').html(d);
                    $('.box-section').val('ManagedBy');
                    SuggestedBox();
                });
            });
        });

        $('#sameHeadOfficeAddress').on('click', function () {
            if ($(this).prop('checked') == true) {
                //alert($('#parentId').val());
                $.get('/secure/office/_GetOfficeInfo?id=' + $('#parentId').val(), function (officeData) {
                    $('#AddressLine1').val(officeData.AddressLine1);
                    $('#AddressLine1').attr('readonly', true);
                    $('#AddressLine2').val(officeData.AddressLine2);
                    $('#AddressLine2').attr('readonly', true);
                    $('#City').val(officeData.City);
