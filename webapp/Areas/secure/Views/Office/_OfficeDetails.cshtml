
<div class="row" style="margin-top:12px;">
    <form id="frm" class="form-Modal" method="post" action="~/Secure/Office/Record">
        @Html.AntiForgeryToken()
        <div class="col-sm-12">
            <div class="col-md-12 text-right">
                <div class="btn-group btn-group-sm" role="group">
                    <button type="button" class="btn btn-xs btn-group-xs btn-default" data-dismiss="modal"><i class="fa fa-close"></i> Cancel</button>
                    <button type="submit" class="btn btn-xs btn-group-xs btn-success"><i class="fa fa-check"></i> Submit</button>
                </div>
            </div>
            <div class="form-group col-md-12" style="display:none;">
                <label><strong>Parent</strong></label>
                <select name="ParentId" id="parentId" class="form-control input-sm">
                    <option value="">-No Parent-</option>
                </select>
            </div>
            <div class="form-group col-md-12" style="display:none">
                <label><strong>Office Type</strong></label>
                <input type="text" name="OfficeType" id="OfficeType" value="" />
            </div>
            <div class="form-group col-md-12">
                <label><strong>Title</strong></label>
                <input name="Title" id="Title" type="text" class="form-control input-sm" value="" required autofocus />
            </div>
            <div class="form-group col-md-12">
                <label><strong>Manager</strong></label>
                <select id="select-ManagedBy" name="ContactId" class="demo-default col-sm-11 col-xs-11 no-padding" required placeholder="Type employees name">
                    <option value="">Type employees name to assign</option>
                    @foreach (var x in ViewBag.AllManagers as IList<BaseApp.Entity.AppUser>)
                    {
                        <option value="@x.Id">@x.FullName</option>
                    }
                </select>
                <a href="javascript://" class="col-sm-1 col-xs-1 crmsearch" title="Search and add more" style="margin-top:3px;"><i class="fa fa-search fa-2x"></i></a>
                <div class="clearfix"></div>
                <div id="crm-box-panel" class="advance-search-box" style="display:none;"><div id="loading-mask-inner" align="center" style="margin: 12% 0;"><img src="~/Content/Images/loader.gif" style="height:60px;" alt="Loading" /></div></div>
            </div>
            <div class="col-md-12">
                <h4 class="form-group panel panel-heading" style="background-color: #8c90ae; color: #fefefe;">
                   <i class="fa fa-globe"></i> Location
                    <span class="pull-right" style="font-size:12px;"><label> <input id="sameHeadOfficeAddress" type="checkbox" /> Use Parent Office Address</label></span>
                </h4>
            </div>
            
            <div class="col-md-12">
                <div class="form-group col-md-6">
                    <label><strong>Country</strong></label>
                    <input name="Country" id="Country" type="text" class="form-control input-sm" value="" required />
                </div>
                <div class="form-group col-md-6">
                    <label><strong>State</strong></label>
                    <input name="State" id="State" type="text" class="form-control input-sm" value="" required />
                </div>
                <div class="form-group col-md-6">
                    <label><strong>City</strong></label>
                    <input name="City" id="City" type="text" class="form-control input-sm" value="" required />
                </div>
                <div class="form-group col-md-6">
                    <label><strong>Zip Code</strong></label>
                    <input name="ZipCode" id="ZipCode" type="text" class="form-control input-sm" value="" required />
                </div>
                <div class="form-group col-md-6">
                    <label><strong>Address Line 1</strong></label>
                    <input name="AddressLine1" id="AddressLine1" class="form-control input-sm" value="" required />
                </div>
                <div class="form-group col-md-6">
                    <label><strong>Address Line 2</strong></label>
                    <input name="AddressLine2" id="AddressLine2" class="form-control input-sm" value="" required />
                </div>
            </div>

            <div class="form-group col-md-6">
                <label><strong>Phone No.</strong></label>
                <input name="PhoneNumber" id="PhoneNumber" class="form-control input-sm" value="" required />
            </div>
            <div class="form-group col-md-6">
                <label><strong>Fax</strong></label>
                <input name="FaxNumber" id="FaxNumber" class="form-control input-sm" value="" required />
            </div>
            <div class="form-group col-md-6">
                <label><strong>Currency</strong></label>
                <select name="Currency" class="form-control input-sm" required>
                    @foreach (var x in Html.CurrenciesList())
                    {
                        <option value="@x.Value">@x.Value - @x.Text</option>
                    }
                </select>
            </div>
            <div class="form-group col-md-6">
