
@{
    ViewBag.Title = "Dashboard";
    var recentPages = ViewBag.RecentPages as List<CMS.Entity.CmsPage>;
    var recentSlides = ViewBag.RecentSlides as List<CMS.Entity.CmsSlide>;
    var recentContents = ViewBag.RecentContents as List<CMS.Entity.CmsContent>;
    var recentNews = ViewBag.RecentNews as List<CMS.Entity.CmsNews>;
    var recentFiles = ViewBag.RecentFiles as List<CMS.Entity.CmsFile>;
}

@section styles{
    <link href="~/Content/radial-progress.less" type="text/css" rel="stylesheet/less" />
    <link href="~/Content/Morris Chart/morris.css" rel="stylesheet" />

    <style>
        .radial-progress {
            margin: 0 auto;
        }

        .dhempbx {
            background-color: #1565c0;
            border: 1px solid #d0d1d4;
            padding: 10px 10px 20px 10px;
            color: #fff !important;
        }

            .dhempbx a {
                color: #fff;
            }

            .dhempbx img {
                height: 135px;
                background-color: #fefefe;
                padding: 4px;
                border-radius: 85px;
            }

            .dhempbx h3 {
                color: #fefefe;
                font-size: 22px;
                font-weight: 500;
                margin: 10px 0 4px 0;
            }

        hr {
            margin-top: 12px;
            margin-bottom: 12px;
        }

        .nwsfdbx {
            display: block;
            position: relative;
            background-color: #fff;
            border: 1px solid #d0d1d4;
            padding: 8px 10px 10px;
            margin: 10px 0 10px 0;
        }

        .nsfdtitle {
            display: block;
            position: relative;
            text-align: left;
            color: #2f2f2f;
            font-weight: bold;
            font-family: Calibri;
            font-size: 14px;
        }

        .ntfcn {
            display: block;
            position: relative;
            text-align: right;
            color: #959595;
        }

        .nwsfdbx .pinkclr {
            background-color: #fff !important;
        }

        .media-left, .media-right {
            vertical-align: top;
        }

        .dbrgtbx {
            background: #fff;
        }

        .dbrgtbx {
            display: block;
            position: relative;
            background-color: #fbfbfb;
            /*border: 1px solid #d0d1d4;*/
            padding: 15px;
        }

        .appstbx {
            display: block;
            position: relative;
            width: 100%;
            border: 1px solid #d0d1d4;
            background-color: #fff;
        }

            .appstbx ul {
                margin-bottom: 0;
            }

        .list-unstyled {
            padding-left: 0;
            list-style: none;
        }

        .appstbx ul li {
            padding: 10px 0;
        }

        .appstbx ul li {
            border-bottom: 1px solid #d0d1d4;
            text-align: center;
            padding: 15px 0;
        }

            .appstbx ul li .apppstjob {
                text-transform: uppercase;
                font-size: 14px;
            }

        .apppstjob {
            color: #717171;
        }

        .list-unstyled a {
            color: #717171;
            display: block;
            text-decoration: none;
        }

            .list-unstyled a:hover {
                text-decoration: none;
                background-color: #1565c0;
                color: #fefefe;
            }

                .list-unstyled a:hover .apppstjob {
                    text-decoration: none;
                    background-color: #1565c0;
                    color: #fefefe;
                }
    </style>
}

